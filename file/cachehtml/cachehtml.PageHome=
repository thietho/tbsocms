<script language="javascript">
$("#btnTraLoi").click(function(){
	//$.blockUI({ message: "<h1>Please wait...</h1>" }); 
	
	$.post("http://localhost:81/tbsocms/?route=sitebar/question/answer", $("#frmQuestion").serialize(),
		function(data){
			if(data == "true")
			{
				$("#question_result").load("http://localhost:81/tbsocms/?route=sitebar/question/viewresult&questionid=default13092464111",function(){
					showPopup("#question_result",500,500,true)		
					
				});
			}
			else
			{
				
				//window.location = "http://localhost:81/tbsocms/site/default/login";
				
				
			}
			//$.unblockUI();
		}
	);					   
});
